    "bump": "cz bump",
    "bump:dr": "cz bump --dry-run",
    "bump:a": "cz bump --prerelease=alpha",
    "bump:b": "cz bump --prerelease=beta",
    "bump:rc": "cz bump --prerelease=rc",
    "bump:dev": "cz bump --devrelease",
    "cl": "cz changelog",
    "cl:dr": "cz changelog --dry-run",
    "build": "rm -rf dist && bun run get && bun run gen && bun run bundle && bun run fonts",
    "get": "rm -rf src && cd scripts && bun --bun make-source.js 'o'",
    "bundle": "concurrently bun:lib:* -m 25 && concurrently bun:shadow:* -m 25",
    "fonts": "concurrently bun:fonts:files:* -m 25 && concurrently bun:fonts:license:* -m 25",
    "fonts:files:atkinson": "cp -r node_modules/@fontsource/atkinson-hyperlegible/files dist/fonts",
    "fonts:license:atkinson": "cp node_modules/@fontsource/atkinson-hyperlegible/LICENSE dist/fonts/atkinson-hyperlegible.LICENSE",
    "gen": "concurrently bun:gen:props bun:gen:shadowdom && bun run gen:extra && bun run gen:types",
    "gen:props": "cd scripts && bun --bun make-props.js 'o' true 'html' 'o'",
    "gen:nowhere": "cd scripts && bun --bun make-props.js 'o' false 'html' 'o'",
    "gen:shadowdom": "cd scripts && bun --bun make-props.js 'o' false ':host' 'o-shadow'",
    "gen:extra": "cd scripts && bun --bun make-extra.js 'o'",
    "gen:types": "tsc -p tsconfig.json",
    "lib:all": "postcss src/index.css -o ./dist/o-props.min.css",
    "lib:normalize": "postcss src/extra/normalize.css -o ./dist/normalize.min.css && bun --bun scripts/shadow-normalize.js",
